{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "declaration": false,
    "skipLibCheck": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "noEmit": true,
    "types": ["vitest/importMeta", "vitest/globals"]
  },
  "exclude": ["node_modules","dist","coverage"]
}
